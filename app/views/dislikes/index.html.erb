<div class="page-header">
  <h1>
    Dislikes
    <a href="/dislikes/new" class="btn btn-lg btn-success">New Dislike</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Smack</th>
        <th>User</th>
        <th>Response</th>
        <th>Actions</th>
      </tr>

      <% @dislikes.each do |dislike| %>
      <tr>
        <td><%= dislike.smack_id %></td>
        <td><%= dislike.user_id %></td>
        <td><%= dislike.response_id %></td>
        <td>
          <a href="/dislikes/<%= dislike.id %>" class="btn btn-primary">Show</a>
          <a href="/dislikes/<%= dislike.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_dislike/<%= dislike.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
